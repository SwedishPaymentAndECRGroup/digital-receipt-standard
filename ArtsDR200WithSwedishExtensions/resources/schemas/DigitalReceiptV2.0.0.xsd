                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="CRIND">
                <xs:annotation>
                    <xs:documentation>Card Reader in Dispenser (Fuel Pump System)</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="URI"/>
        </xs:restriction>
    </xs:simpleType>
